<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111">
   <children>
      <ScrollPane prefHeight="600.0" prefWidth="800.0">
         <content>
            <AnchorPane prefHeight="1000.0" prefWidth="780.0">
               <children>
                  <VBox layoutX="14.0" layoutY="14.0" prefHeight="322.0" prefWidth="270.0" spacing="5.0">
                     <children>
                         <Label fx:id="name" styleClass="cell_small_label" text="\$name" />
                         <Label fx:id="phone" styleClass="cell_small_label" text="\$phone" />
                         <Label fx:id="address" styleClass="cell_small_label" text="\$address" />
                         <Label fx:id="dob" styleClass="cell_small_label" text="\$dob" />
                         <Label fx:id="email" styleClass="cell_small_label" text="\$email" />
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
         </content>
      </ScrollPane>
   </children>
</StackPane>
